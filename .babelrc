{
    "plugins": [
      "syntax-dynamic-import",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread",
      [
        "transform-runtime",
        {
          "polyfill": false
        }
      ]
    ],
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "IE 11"
            ]
          },
          "modules": false
        }
      ]
    ],
    "sourceMaps": "inline",
    "env": {
      "server": {
        "plugins": [
          "syntax-dynamic-import",
          "transform-class-properties",
          "transform-export-extensions",
          "transform-object-rest-spread",
          [
            "transform-runtime",
            {
              "polyfill": false
            }
          ]
        ],
        "presets": [
          "es2015"
        ],
        "sourceMaps": "inline"
      }
    }
  }